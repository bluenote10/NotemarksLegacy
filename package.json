{
  "name": "notemarks",
  "version": "0.1.0",
  "description": "Simple note taking app.",
  "main": "src/app_main.js",
  "scripts": {
    "build-all": "npm run build && npm run build-css",
    "build-css": "node-sass --omit-source-map-url styles/styles.scss build/styles.css",
    "watch-css": "npm run build-css && npm run build-css -- -w",
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "electron .",
    "check-types": "tsc"
  },
  "keywords": [
    "Electron",
    "Note taking"
  ],
  "author": "Fabian Keller",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@types/highlight.js": "^9.12.3",
    "@types/js-yaml": "^3.12.1",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.6.8",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-dom-expressions": "^0.11.2",
    "bulma": "^0.7.5",
    "electron": "^9.1.0",
    "eslint": "^6.1.0",
    "node-sass": "^4.12.0",
    "showdown": "^1.9.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "css-loader": "^3.1.0",
    "electron-localshortcut": "^3.1.0",
    "glob": "^7.1.4",
    "highlight.js": "^9.15.8",
    "js-yaml": "^3.13.1",
    "monaco": "^1.201704190613.0",
    "monaco-editor": "^0.17.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "mousetrap": "^1.6.3",
    "solid-js": "^0.10.2",
    "style-loader": "^0.23.1"
  }
